{
  "name": "nmusic2",
  "version": "1.0.0-dev",
  "description": "Node music player",
  "main": "index.js",
  "scripts": {
    "preinstall": "rm -rf ./node_modules",
    "postinstall": "NODE_ENV=production ./node_modules/.bin/webpack",
    "start": "NODE_ENV=production node ./api/index.js",
    "dev:api": "NODE_ENV=development nodemon -w ./api ./api/index.js",
    "dev:wds": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server",
    "scan_music": "node ./scripts/scan-music.js",
    "test": "./test.sh"
  },
