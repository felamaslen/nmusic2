  },
  "homepage": "https://github.com/felamaslen/nmusic2#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.8.0",
    "mocha": "^4.0.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.2",
